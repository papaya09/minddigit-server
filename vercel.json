{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["sin1"],
  "builds": [
    {
      "src": "test-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/test-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/test-server.js"
    }
  ]
} 